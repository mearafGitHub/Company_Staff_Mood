<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gebeya_mood">

    <application
        android:allowBackup="true"
        android:icon="@drawable/gebeya_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gebeya_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MoodSubmit"></activity>
        <activity android:name=".MyMoodsActivity"
            android:label="@string/title_activity_my_moods"/>
        <activity android:name=".HomeActivity"
            android:label="@string/title_activity_home"/>
        <activity android:name=".AdminActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MoodPromptActivity"
            android:label="@string/title_activity_mood_prompt"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity"
            android:label="@string/action_sing_up_short"/>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
    </application>

</manifest>