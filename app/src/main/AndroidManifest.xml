<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gebeya_mood">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/gebeya_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gebeya_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".moods.MoodSubmit"></activity>
        <activity android:name=".mymoods.MyMoodsActivity"
            android:label="@string/title_activity_my_moods"/>
        <activity android:name=".moodhome.MoodsHomeActivity"
            android:label="@string/title_activity_home"/>
        <activity android:name=".admin.AdminActivity"
            android:label="@string/admin_title"/>
        <activity android:name=".LoginActivity" />
        <activity android:name=".moods.MoodPromptActivity"
            android:label="@string/title_activity_mood_prompt"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signup.SignUpActivity"
            android:label="@string/action_sing_up_short"/>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
    </application>

</manifest>